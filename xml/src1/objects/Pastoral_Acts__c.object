    <fields>
        <fullName>Report_PA_Age__c</fullName>
        <description>Reports to get the Age for Act performed Age</description>
        <externalId>false</externalId>
        <formula>IF(  ISPICKVAL( Act_Performed__c , &apos;Baptism&apos;) &amp;&amp;  ISPICKVAL( Baptism_Age__c , &apos;Infant/Youth&apos;)  &amp;&amp;  NOT( ISBLANK( Date_of_Birth__c ) ) 
, FLOOR((TODAY() - Date_of_Birth__c +1)/365.2425)
, null)</formula>
        <label>Age</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Report_ProfessionOfFaith_Years__c</fullName>
        <description>Profession Of Faith Act performed Years</description>
        <externalId>false</externalId>
        <formula>IF(  ISPICKVAL( Act_Performed__c , &apos;Profession of Faith&apos;)  &amp;&amp;  NOT( ISBLANK(  Date_Act_Performed__c  ) ) 
, FLOOR((TODAY() -  Date_Act_Performed__c +1)/365.2425)
, null)</formula>
        <label>ProfessionOfFaith Years</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Report_Transfer_years_completed__c</fullName>
        <description>Report,to count the number of years fot Act performed Transfer</description>
        <externalId>false</externalId>
        <formula>IF(  ISPICKVAL( Act_Performed__c , &apos;Transfer&apos;)  &amp;&amp;  NOT( ISBLANK(  Date_Act_Performed__c  ) ) 
, FLOOR((TODAY() - Date_Act_Performed__c  +1)/365.2425)
, null)</formula>
        <label>Transfer years completed</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>

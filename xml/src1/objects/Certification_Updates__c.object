    <fields>
        <fullName>Renewal_Year__c</fullName>
        <description>This is the year the certification should be renewed based on Annual Conference Approved Date and Renewed Date.</description>
        <externalId>false</externalId>
        <formula>IF(OR( ISPICKVAL(Year__c,&apos;Missing&apos;) , ISPICKVAL(Year__c,&apos; &apos;))
,IF(ISBLANK(Annual_Conference_Approved_Date__c)
,NULL
, IF(MOD(YEAR(Annual_Conference_Approved_Date__c) + 2, 2) = 0
,TEXT(YEAR(Annual_Conference_Approved_Date__c) + 2)
,TEXT(YEAR(Annual_Conference_Approved_Date__c) + 1)
))
,IF(MOD(VALUE(TEXT(Year__c ))+ 2, 2) = 0
 ,TEXT(VALUE(TEXT(Year__c)) + 2)
  ,TEXT(VALUE(TEXT(Year__c)) + 1)
))</formula>
        <inlineHelpText>This is the year the certification should be renewed based on Annual Conference Approved Date and Renewed Date.</inlineHelpText>
        <label>Renewal Year</label>
        <required>false</required>
        <trackHistory>false</trackHistory>

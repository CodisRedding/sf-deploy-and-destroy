    <validationRules>
        <fullName>WithDrawn_Reason_OtherSpecify</fullName>
        <active>true</active>
        <description>If the &apos;Wothdrawn Reason&apos; is &apos;Others/Specify&apos; .Then user should enter a value in the &apos;Withdrawn Reason (Other/specify)&apos;</description>
        <errorConditionFormula>AND( ISPICKVAL(   Withdrawn_Reason__c    ,&quot;Other/Specify&quot;) , ISBLANK(    Withdrawn_Reason_Other_Specify__c    ) )</errorConditionFormula>
        <errorDisplayField>Withdrawn_Reason_Other_Specify__c</errorDisplayField>
        <errorMessage>&apos;Withdrawn Reason(Other/Specify)&apos; may contain a value if Withdrawn Reason is &apos;Other/Specify&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WithdrawnReason_OtherSpecify_WD_OS</fullName>
        <active>true</active>
        <description>If Withdrawn Reason is not &apos;Other/Specify&apos; then Withdrawn Reason(Other/Specify) Should not contain a value</description>
        <errorConditionFormula>AND(NOT( ISPICKVAL(   Withdrawn_Reason__c   ,&quot;Other/Specify&quot;)) ,NOT( ISBLANK(   Withdrawn_Reason_Other_Specify__c ) ))</errorConditionFormula>
        <errorDisplayField>Withdrawn_Reason_Other_Specify__c</errorDisplayField>
        <errorMessage>&apos;Withdrawn Reason(Other/Specify)&apos; may not contain a value if Withdrawn Reason is NOT &apos;Other/Specify&apos;</errorMessage>
    </validationRules>

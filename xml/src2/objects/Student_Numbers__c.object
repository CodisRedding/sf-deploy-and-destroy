    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>YearEstablished_Year</fullName>
        <active>true</active>
        <description>Year should be Equal or greater than Year Established</description>
        <errorConditionFormula>IF (NOT (ISNULL( VALUE( Year__c))),   VALUE(School_Organization__r.Year_Established__c)    &gt;    VALUE(Year__c)   ,FALSE)</errorConditionFormula>
        <errorDisplayField>Year__c</errorDisplayField>
        <errorMessage>Year Should be Greater Than or Equal to School Year Established</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_NotLess_4</fullName>
        <active>true</active>
        <description>Year should not be less than 4 Numbers</description>
        <errorConditionFormula>AND((Year__c &lt;&gt; null),LEN(Year__c ) &lt; 4)</errorConditionFormula>
        <errorDisplayField>Year__c</errorDisplayField>
        <errorMessage>&apos;Year&apos; should not contain a value less than 4 Numbers</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_Number</fullName>
        <active>true</active>
        <description>Year should be only Number</description>
        <errorConditionFormula>AND(( Year__c  &lt;&gt; null),  
NOT(ISNUMBER( Year__c )))</errorConditionFormula>
        <errorDisplayField>Year__c</errorDisplayField>
        <errorMessage>Only Year(Number) should be entered</errorMessage>
    </validationRules>
</CustomObject>

    <deploymentStatus>Deployed</deploymentStatus>
    <description>Organization number of students for each year</description>
    <enableActivities>true</enableActivities>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableReports>true</enableReports>
    <fields>
        <fullName>Number_Of_Students__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Enter number of students enrolled at institution during specified enrollment year</inlineHelpText>
        <label>Number Of Students</label>
        <precision>10</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>School_Organization__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Number of students in a particular year in an Organization School</description>
        <externalId>false</externalId>
        <label>School(Organization)</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Student Numbers</relationshipLabel>
        <relationshipName>SchoolsYearStudents</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Year__c</fullName>
        <description>Year Number</description>
        <externalId>false</externalId>
        <inlineHelpText>Enter Year Number</inlineHelpText>
        <label>Fall Enrollment Year</label>
        <length>4</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Student Numbers</label>
    <nameField>
        <displayFormat>SN - {0000}</displayFormat>
        <label>Student Number</label>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Student Numbers</pluralLabel>
    <searchLayouts>

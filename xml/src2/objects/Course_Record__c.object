        <fullName>Amount_Other_Specify_Required_Rule_2</fullName>
        <active>true</active>
        <description>A value may be entered in &apos;Amount (Other/Specify)&apos; Only when the Value of &apos;Amount&apos; is set to &apos;Other/Specify&apos;</description>
        <errorConditionFormula>NOT(ISNULL( Amount_Other_Specify__c )) &amp;&amp; NOT(ISPICKVAL( Amount_Paid__c , &apos;Other/Specify&apos;))</errorConditionFormula>
        <errorDisplayField>Amount_Other_Specify__c</errorDisplayField>
        <errorMessage>A value may be entered in &apos;Amount (Other/Specify)&apos; Only when the Value of &apos;Amount&apos; is set to &apos;Other/Specify&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Amount_Paid_Is_Not_Other_Specify_Rule</fullName>
        <active>true</active>
        <description>If Amount Paid is not Other/Specify then set Amount Paid (Other/Specify) to null.</description>
        <errorConditionFormula>ISPICKVAL(Amount_Paid__c , &apos;Other/Specify&apos;) &amp;&amp; ISNULL(Amount_Other_Specify__c )</errorConditionFormula>
        <errorDisplayField>Amount_Other_Specify__c</errorDisplayField>
        <errorMessage>&apos;Amount (Other/Specify)&apos; must contain a value when amount paid is &apos;Other/Specify&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_Cannot_Change_Rule</fullName>

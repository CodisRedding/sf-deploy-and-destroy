    <validationRules>
        <fullName>AppDate_Before_GBHEMApproved_Date_Rule</fullName>
        <active>true</active>
        <description>If Application Date and GBHEMApproval Date are entered then Application Date must be prior to Approval Date.</description>
        <errorConditionFormula>(AND(NOT(ISBLANK(Application_Date__c)), NOT(ISBLANK
(GBHEM_Approved_Date__c  ))) &amp;&amp;
(GBHEM_Approved_Date__c  &lt; Application_Date__c))</errorConditionFormula>
        <errorDisplayField>GBHEM_Approved_Date__c</errorDisplayField>
        <errorMessage>If Application Date and GBHEMApproval Date are entered then Application Date must be prior to Approval Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GBHEMApprovedDate_VAlidation</fullName>
        <active>true</active>
        <description>GBHEM Approved date should not be a future Date</description>
        <errorConditionFormula>IF (NOT (ISNULL(  GBHEM_Approved_Date__c )),  GBHEM_Approved_Date__c  &gt; TODAY() ,FALSE)</errorConditionFormula>
        <errorDisplayField>GBHEM_Approved_Date__c</errorDisplayField>
        <errorMessage>GBHEM Approved Date should be Today or Before Today.</errorMessage>
    </validationRules>

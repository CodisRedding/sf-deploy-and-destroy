<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Store default values for determining runtime behavior.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Developers_Chatter_Group_Default_Member__c</fullName>
        <defaultValue>&quot;rassad.sfadmin@gbhem.org.rocky&quot;</defaultValue>
        <description>The default users email address used to query the user name in this org.</description>
        <externalId>false</externalId>
        <inlineHelpText>The default users email address used to query the user name in this org.</inlineHelpText>
        <label>Developers Chatter Group Default Member</label>
        <length>100</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Developers_Chatter_Group_Name__c</fullName>
        <defaultValue>&quot;Developers&quot;</defaultValue>
        <description>The name of the chatter group that developers are added to so that they receive system errors as they occur.</description>
        <externalId>false</externalId>
        <inlineHelpText>The name of the chatter group that developers are added to so that they receive system errors as they occur.</inlineHelpText>

        <externalId>false</externalId>
        <inlineHelpText>Stops record errors from being posted to the record owners via chatter.</inlineHelpText>
        <label>Suppress Record Errors to Chatter</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Suppress_System_Errors_to_Chatter__c</fullName>
        <defaultValue>false</defaultValue>

        <externalId>false</externalId>
        <inlineHelpText>Stops system errors from being posted to the developers chatter group.</inlineHelpText>
        <label>Suppress System Errors to Chatter</label>
        <type>Checkbox</type>
    </fields>
    <label>Global App Settings</label>
</CustomObject>
